create table daily_task (
status number not null,task varchar2(50) not null,done_by varchar2(30) not null,deadline date not null,completed_on date)
constraint number_cq check (number in(1,0),
constraint completed_on_cq check (completed_on <= deadline)
);

insert into daily_task (
status,task,done_by,deadline,compleated_on) values (1,dailyTask1,soundarya,
to_date('28-12-2019','dd-MM-yyyy'),to_date('27-12-2019','dd-MM-yyyy')
);

insert into daily_task (
status,task,done_by,deadline,compleated_on) values (0,dailyTask2,nandhu,
to_date('28-12-2019','dd-MM-yyyy')
);

select * from daily_task where status = 1;
